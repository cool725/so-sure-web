                                 id="date_time_picker"
                                 data-target-input="nearest"
                                 data-url="">
                                <input type="text"
                                       class="form-control datetimepicker-input"
                                       data-target="#date_time_picker"
                                       value="{{ now|date('d-m-Y') }}" />
                                <div class="input-group-append"
                                     data-target="#date_time_picker"
                                     data-toggle="datetimepicker">
                                    <div class="input-group-text"><i class="fal fa-calendar fa-fw"></i></div>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <button class="btn btn-success btn-square mr-2">
                                <i class="fa fa-arrow-right fa-fw"></i>
                            </button>
                        </div>
                        <div class="form-group">
                            <a href="{{ previous_page }}" class="btn btn-primary btn-square mr-2">
                                <i class="fa fa-arrow-left fa-fw"></i>
                            </a>
                        </div>
                        <div class="form-group">
                            <a href="{{ next_page }}" class="btn btn-primary btn-square">
