{% extends '::base.html.twig' %}

{% block title %}so-sure - Server Error{% endblock %}

{% block remotestylesheets %}
    <link rel="stylesheet" href="/css/error.css">
{% endblock %}

{% block body %}
<section class="error-page">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
		      <h1 class="section-title">Sorry, computer says, no.</h1>
		      <img class="logo" src="{{ cdn_url }}/images/logo/unhappy-so-sure-smiley.svg" alt="Unhappy so-sure smiley"></img>
		      <p class="lead">It looks like there was a server error. Our engineers will investigate the problem, but in the meantime, try refreshing the page.</p>
            </div>
        </div>
    </div>
</section>
{% endblock %}
