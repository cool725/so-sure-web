{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { "": "src/" },
        "files": [ "app/AppKernel.php" ]
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "~1.3",
        "hwi/oauth-bundle": "^0.4.0",
        "symfony/assetic-bundle": "^2.7",
        "drewm/mailchimp-api": "dev-api-v3",
        "google/apiclient": "^2.0.0@RC",
        "staffim/rollbar-bundle": "*",
        "judopay/judopay-sdk": "1.0.3.1",
        "facebook/php-sdk-v4": "^5.1",
        "aws/aws-sdk-php": "^3.15",
        "jms/security-extra-bundle": "^1.6",
        "white-october/pagerfanta-bundle": "^1.0",
        "geoip2/geoip2": "~2.0",
        "jmikola/geojson": "~1.0",
        "gocardless/gocardless-pro": "^0.9.3",
        "plivo/plivo-php": "^1.1",
        "phpoffice/phpexcel": "^1.8",
        "lcobucci/jwt": "^3.1",
        "snc/redis-bundle": "^1.1",
        "predis/predis": "1.0.4",
        "egulias/email-validator": "~1.2",
        "fzaninotto/faker": "^1.6",
        "stof/doctrine-extensions-bundle": "^1.2",
        "vich/uploader-bundle": "^1.1.0",
        "oneup/flysystem-bundle": "^1.4.1",
        "league/flysystem-aws-s3-v3": "^1.0.13",
        "alcaeus/mongo-php-adapter": "^1.0",
        "doctrine/mongodb-odm": "^1.1",
        "doctrine/mongodb-odm-bundle": "^3.2",
        "knplabs/knp-snappy-bundle": "^1.4"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7",
        "phing/phing": "^2.13",
        "phpunit/phpunit": "^4.3",
        "liip/functional-test-bundle": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "squizlabs/php_codesniffer": "2.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/so-sure/PhpSdk.git"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
