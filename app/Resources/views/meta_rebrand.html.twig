<meta name="msapplication-TileColor" content="#ffffff">
<meta name="msapplication-TileImage" content="{{ cdn_url }}/images/rebrand/favicons/ms-icon-144x144.png">

<meta name="theme-color" content="#0051E8">
<meta name="msapplication-navbutton-color" content="#0051E8">
<meta name="apple-mobile-web-app-status-bar-style" content="#0051E8">

<meta name="msapplication-square70x70logo" content="{{ cdn_url }}/images/rebrand/favicons/ms-icon-70x70.png" />
<meta name="msapplication-square150x150logo" content="{{ cdn_url }}/images/rebrand/favicons/ms-icon-150x150.png" />
<meta name="msapplication-square310x310logo" content="{{ cdn_url }}/images/rebrand/favicons/ms-icon-310x310.png" />

<meta property="fb:app_id" content="{{ fb_appid }}" />
<meta property="og:type" content="website" />

{% if og_url is defined %}
    <meta property="og:url" content="{{ og_url }}" />
{% else %}
    <meta property="og:url" content="{{ app.request.uri }}" />
{% endif %}

{% if og_title is defined %}
  <meta property="og:title" content="{{ og_title }}" />
{% else %}
  <meta property="og:title" content="Welcome to Social Insurance" />
{% endif %}

{% if og_description is defined %}
    <meta property="og:description" content="{{ og_description }}" />
{% else %}
    <meta property="og:description" content="The future of phone insurance has arrived (and it's up to 80% cheaper when you connect with friends & don't claim)." />
{% endif %}

{% if og_image is defined %}
  <meta property="og:image" content="{{ og_image }}" />
{% else %}
  <meta property="og:image" content="{{ cdn_url }}/images/rebrand/og/so-sure_og-image-2.png" />
{% endif %}

<meta property="fb:pages" content="781483845311300" />

<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="@wearesosure" />

{% if og_image is defined %}
    <meta name="twitter:image" content="{{ og_image }}" />
{% else %}
    <meta name="twitter:image" content="{{ cdn_url }}images/rebrand/og/so-sure_og-image-2.png" />
{% endif %}
