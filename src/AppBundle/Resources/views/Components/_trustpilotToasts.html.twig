<div class="toast toast-1 {{ toast_position }}"
     role="alert"
     data-autohide="true"
     data-delay="5000">
    <div class="toast-body">
        <div class="toast-review d-flex align-items-start"
             vocab="https://schema.org/"
             typeof="Review">
            <span property="itemReviewed"
                  typeof="Thing">
                <meta property="name" content="so-sure mobile insurance">
            </span>
            <span property="publisher" typeof="Organization">
                <meta property="name" content="Trustpilot">
            </span>
            <span class="review__rating"
                  property="reviewRating"
                  typeof="Rating">
                <meta property="ratingValue" content="5">
                <meta property="bestRating" content="5">
                <img src="{{ cdn_url }}/images/onexone.png" data-src="{{ cdn_url }}/images/rebrand/reviews/so-sure_reviews-female-gravatar.png" alt="so-sure customer" class="lozad toast-review-img mr-3" width="50px">
            </span>
            <div class="toast-review-text">
                <img src="{{ cdn_url }}/images/onexone.png" data-src="{{ cdn_url }}/images/rebrand/toasts/so-sure_toast_trustpilot_rating_5.svg" class="lozad" width="90px">
                <div class="h6 mt-2 mb-1"
                    property="author"
                    typeof="Person">
                    <span property="name">Lisa</span>
                </div>
                <spam property="description">Really good after putting on details of everything I needed got a email validate my phone condition really clever after doing that if your phone is perfect your excess drops</spam>
            </div>
        </div>
    </div>
</div>

