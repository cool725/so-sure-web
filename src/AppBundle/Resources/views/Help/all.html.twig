{% extends 'base.html.twig' %}

{% block intercom_enabled %}{% if intercom_enabled %}true{% else %}false{% endif %}{% endblock %}

{% block title %}
    {{ title }}
{% endblock %}

{% block metaend %}
    {{ head|raw }}
{% endblock %}

{% block nav %}
    {% if not hide_nav %}
        {% include '::nav.html.twig' %}
    {% endif %}
{% endblock %}

{% block body %}
 {{ body|raw }}
{% endblock %}

{% block footer %}
    {% if not hide_footer %}
        {% include '::footer.html.twig' %}
    {% endif %}
{% endblock %}
